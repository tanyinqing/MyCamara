<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    tools:context="glidedisanfang.tyq.com.tanaar_171207.activity.DevelopActivity">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="10dp"
       android:background="@android:color/transparent" />
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_information"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge" />

            <glidedisanfang.tyq.com.tanaar_171207.customview.MyListView
                android:id="@+id/list_data"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp">
            </glidedisanfang.tyq.com.tanaar_171207.customview.MyListView>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/btn_word_delete"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="onClick"
                    android:layout_margin="5dp"
                    android:text="work_delete" />
                <Button
                    android:id="@+id/kill_process"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="onClick"
                    android:layout_margin="5dp"
                    android:text="kill_process" />
                <Button
                    android:id="@+id/delete_db"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="onClick"
                    android:layout_margin="5dp"
                    android:text="delete_db" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <Button
                    android:id="@+id/cache_data"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="onClick"
                    android:layout_margin="5dp"
                    android:text="cache_data" />
                <Button
                    android:id="@+id/list_datas"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="onClick"
                    android:layout_margin="5dp"
                    android:text="list_datas" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>


</LinearLayout>
